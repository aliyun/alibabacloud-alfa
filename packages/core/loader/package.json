{
  "name": "@alicloud/console-os-loader",
  "version": "1.4.32",
  "description": "Console OS Loader",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prepublish": "rm -rf ./lib ./es && npm run build && npm run babel && npm run babel:esm",
    "babel": "breezr build --engine babel && tsc --emitDeclarationOnly -d --declarationDir ./lib",
    "babel:esm": "breezr build --engine babel --es-module",
    "build": "breezr build --engine webpack",
    "storybook": "breezr start-storybook",
    "clean": "rm -rf lib yarn.lock"
  },
  "devDependencies": {
    "@alicloud/console-toolkit-cli": "^1.0.0",
    "@alicloud/console-toolkit-preset-wind-component": "^1.0.0",
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.195",
    "jest": "^23.6.0",
    "serve-static": "^1.14.1",
    "ts-jest": "^23.10.5",
    "typescript": "^4.0.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "tslib": "^2.4.0"
  }
}
