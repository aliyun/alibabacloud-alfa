{
  "name": "@alicloud/alfa-core",
  "version": "1.4.0-alpha.4",
  "description": "MicroFront End SDK for alicloud",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "author": "Boelroy",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublish": "npm run build && npm run babel && npm run babel:esm",
    "babel": "breezr build --engine babel && tsc --emitDeclarationOnly -d true --declarationDir ./lib",
    "babel:esm": "breezr build --engine babel --es-module",
    "build": "breezr build --engine webpack",
    "storybook": "breezr start-storybook",
    "clean": "rm -rf lib yarn.lock"
  },
  "devDependencies": {
    "@alicloud/console-toolkit-cli": "^1.0.0",
    "@alicloud/console-toolkit-preset-wind-component": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.13",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@alicloud/console-logger-sls": "^1.2.31",
    "@alicloud/console-os-kernal": "^1.4.0-alpha.1"
  },
  "gitHead": "9c6fffdb53e1b42c98b80e3f4eb896a3e2ee0c86"
}
